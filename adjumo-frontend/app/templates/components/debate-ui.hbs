<td class="debate-venue">
    <small>{{ debate.venue }}</small>
</td>
<td class="debate-points text-center">
    {{ debate.points }}
</td>
<td class="debate-importance">
    {{ input type="number" value=debate.importance }}
</td>
<td class="debate-og text-center  gender-{{ debate.og.gender }} region-{{ debate.og.region }}">
    {{ debate.og.name }}
</td>
<td class="debate-oo text-center  gender-{{ debate.oo.gender }} region-{{ debate.oo.region }}">
    {{ debate.oo.name }}
</td>
<td class="debate-cg text-center  gender-{{ debate.cg.gender }} region-{{ debate.cg.region }}">
    {{ debate.cg.name }}
</td>
<td class="debate-co text-center  gender-{{ debate.co.gender }} region-{{ debate.co.region }}">
    {{ debate.co.name }}
</td>
<td class="debate-panel">
    {{panel-ui panel=debate.panel drake=drake }}
</td>
