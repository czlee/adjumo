
<!-- Modal -->
<div class="modal fade" id="setAllocationParameters" tabindex="-1" role="dialog" aria-labelledby="parametersModal">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="setAllocationParameters">Set Allocation Parameters</h4>
      </div>
      <div class="modal-body">
        <form>
          <fieldset class="form-group row">
            <label for="adjconflict_fader" class="col-sm-2">Quality</label>
            <div class="col-sm-8">
              {{input type="range" min="0" max="10" step="1" list="parameter-ticks" value=config.quality  }}
            </div>
            <div class="col-sm-1">
                {{input class="form-control"                  value=config.quality}}
            </div>
          </fieldset>
          <fieldset class="form-group row">
            <label for="adjconflict_fader" class="col-sm-2">Regional</label>
            <div class="col-sm-8">
              {{input type="range" min="0" max="10" step="1" list="parameter-ticks" value=config.regional  }}
            </div>
            <div class="col-sm-1">
                {{input class="form-control"                  value=config.regional}}
            </div>
          </fieldset>
          <fieldset class="form-group row">
            <label for="adjconflict_fader" class="col-sm-2">Language</label>
            <div class="col-sm-8">
              {{input type="range" min="0" max="10" step="1" list="parameter-ticks" value=config.language }}
            </div>
            <div class="col-sm-1">
                {{input class="form-control"                  value=config.language}}
            </div>
          </fieldset>
          <fieldset class="form-group row">
            <label for="adjconflict_fader" class="col-sm-2">Gender</label>
            <div class="col-sm-8">
              {{input type="range" min="0" max="10" step="1" list="parameter-ticks" value=config.gender }}
            </div>
            <div class="col-sm-1">
                {{input class="form-control"                  value=config.gender}}
            </div>
          </fieldset>
          <fieldset class="form-group row">
            <label for="adjconflict_fader" class="col-sm-2">Team History</label>
            <div class="col-sm-8">
              {{input type="range" min="0" max="10" step="1" list="parameter-ticks" value=config.teamhistory }}
            </div>
            <div class="col-sm-1">
                {{input class="form-control" value=config.teamhistory}}
            </div>
          </fieldset>
          <fieldset class="form-group row">
            <label for="adjconflict_fader" class="col-sm-2">Adj History</label>
            <div class="col-sm-8">
              {{input type="range" min="0" max="10" step="1" list="parameter-ticks" value=config.adjhistory }}
            </div>
            <div class="col-sm-1">
                {{input class="form-control" value=config.adjhistory}}
            </div>
          </fieldset>
          <fieldset class="form-group row">
            <label for="adjconflict_fader" class="col-sm-2">Team Conflict</label>
            <div class="col-sm-8">
              {{input type="range" min="0" max="10" step="1" list="parameter-ticks" value=config.teamconflict }}
            </div>
            <div class="col-sm-1">
                {{input class="form-control" value=config.teamconflict}}
            </div>
          </fieldset>
          <fieldset class="form-group row">
            <label for="adjconflict_fader" class="col-sm-2">Adj Conflict</label>
            <div class="col-sm-8">
              {{input type="range" min="0" max="10" step="1" list="parameter-ticks" value=config.adjconflict }}
            </div>
            <div class="col-sm-1">
                {{input class="form-control" value=config.adjconflict}}
            </div>
          </fieldset>
          <fieldset class="form-group row">
            <label for="adjconflict_fader" class="col-sm-2">α-Fairness</label>
            <div class="col-sm-8">
              {{input type="range" min="0" max="10" step="1" list="parameter-ticks" value=config.α }}
            </div>
            <div class="col-sm-1">
                {{input class="form-control" value=config.α}}
            </div>
          </fieldset>
          <fieldset class="form-group row">
            <label for="adjconflict_fader" class="col-sm-2"> </label>
            <div class="col-sm-8">
              <button type="submit" class="btn btn-success btn-block" {{action "saveConfig"}}>
                Save Configuration
              </button>
            </div>
            <datalist id="parameter-ticks">
                <option>0</option>
                <option>1</option>
                <option>2</option>
                <option>3</option>
                <option>4</option>
                <option>5</option>
                <option>6</option>
                <option>7</option>
                <option>8</option>
                <option>9</option>
                <option>10</option>
            </datalist>
          </fieldset>
        </form>
      </div>
    </div>
  </div>
</div>